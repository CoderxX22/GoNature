<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Region?>

<VBox alignment="CENTER" spacing="15" style="-fx-background-color: #f0f0f0;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SearchOrderFrameController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <children>
        <Label fx:id="idLbl" text="Order number:" />
        <TextField fx:id="Ordertxt" />
        <Region minWidth="0" prefWidth="0" />
        <Button fx:id="btnSearch" onAction="#Search" text="Search" />
        <Button fx:id="btnExit" onAction="#getExitBtn" text="Exit" />
		<Label fx:id="errorMsg" layoutX="166.0" layoutY="300.0" prefHeight="17.0" prefWidth="250.0" text="" />
    </children>
</VBox>
