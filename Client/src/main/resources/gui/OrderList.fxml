<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="400.0" prefWidth="500" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.OrderList">
   <VBox prefHeight="400.0" prefWidth="1071.0">
      <TableView fx:id="orderTable" editable="true" prefHeight="375.0" prefWidth="803.0">
         <columns>
            <TableColumn fx:id="parkId" minWidth="0.0" prefWidth="44.0" text="Park" />
            <TableColumn fx:id="visitTime" prefWidth="62.0" text="Visit Time" />
            <TableColumn fx:id="visit_date" prefWidth="74.0" text="Visit Date" />
            <TableColumn fx:id="numVisitors" prefWidth="118.0" text="Number of Visitors" />
            <TableColumn fx:id="email" editable="false" prefWidth="201.0" text="Email" />
         </columns>
      </TableView>
      <HBox prefHeight="25.0" prefWidth="646.0" spacing="10">
         <Button fx:id="saveButton" text="Save Changes" />
         <Button fx:id="backButton" text="Back" />
         <Label fx:id="saveMessage" />
      </HBox>
   </VBox>
</StackPane>
