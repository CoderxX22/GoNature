<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainWindow">
   <left>
      <VBox prefHeight="400.0" prefWidth="140.0" style="-fx-background-color: #Caffd1" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="profilePicture" fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></ImageView>
            <Label fx:id="name" alignment="CENTER" prefHeight="20.0" prefWidth="140.0" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <font>
                  <Font name="Segoe UI Black" size="18.0" />
               </font></Label>
            <VBox fx:id="options_list_vbox" prefHeight="254.0" prefWidth="140.0" style="-fx-background-color: #Caffd1" BorderPane.alignment="CENTER" />
         </children></VBox>
   </left>
   <center>
      <StackPane fx:id="main_hbox" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
